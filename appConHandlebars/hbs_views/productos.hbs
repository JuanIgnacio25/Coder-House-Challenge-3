<h1>Productos</h1>
{{#if productos}}
<table>
    <thead>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Thumbnail</th>
    </thead>
    <tbody>
        {{#each productos}} 
        <tr>
            <td>{{ this.name }}</td>
            <td>$ {{ this.price }}</td>
            <td><img style="width: 50px; height:50px" src={{this.thumbnail}} alt=""></td>
        </tr>
   
        {{/each}}
    </tbody>
</table>
{{else}}
<h2>No hay productos</h2>
{{/if}}
<div class="container-botom">
    <button type="button">
        <nav>
            <a href="http://localhost:8080/">Listado de Productos</a>
        </nav>
    </button>
</div>